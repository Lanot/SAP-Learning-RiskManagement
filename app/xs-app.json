{
  "welcomeFile": "/app/risks/webapp/index.html",
  "routes": [
    {
      "source": "^/app/(.*)$",
      "localDir": "./",
      "target": "$1",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/service/(.*)$",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
    }
  ]
}